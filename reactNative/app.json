{
  "expo": {
    "name": "momuletV2",
    "slug": "momuletV2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["kakaomap"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "exp"
  }
}
