{
  "expo": {
    "name": "momelet",
    "slug": "momelet",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.recoder.momelet",
      "supportsTablet": false,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["kakaomap"]
      },
      "buildNumber": "1.0.3"
    },

    "android": {
      "package": "com.recoder.momelet",
      "versionCode": 1
    },

    "scheme": "momelet"
  }
}
